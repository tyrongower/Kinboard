{
  "expo": {
    "name": "Kinboard",
    "slug": "kinboard",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b1220"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b1220"
      },
      "tvBanner": "./assets/adaptive-icon.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "524accc2-4549-46ac-995e-6bbfd5af5c18"
      }
    },
    "owner": "tyron-gower",
    "plugins": [
      "expo-font",
      "./plugins/withAndroidTv"
    ]
  }
}
